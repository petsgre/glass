page {
  background: #feefff;
}
.contain {
  width: 750rpx;
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
  color: #fff;
  height: 400rpx;
  box-shadow: 0 0 20rpx 2px #fff inset;
}

.background {
  width: 100%;
  top: 0;
  position: absolute;
  z-index: -12;
  filter: blur(2px);
  height: 400rpx;
  opacity: 0.8;
  display: block;
}
.contain::before {
  
  content: '';
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.2);
  z-index: -1;
}
.headers {
  width: 120rpx;
  height: 120rpx;
  border: 2rpx solid #776545;
  border-radius: 100rpx;
  overflow: hidden;
}
.headers image {
  width: 100%;
  height: 100%;
  filter: sepia(0.77);
}
.name {
  font-size: 46rpx;
  margin-top: 30rpx;
}
.position {
  font-size: 24rpx;
  width: 200rpx;
  margin: 10rpx 0;
  text-align: center;
  display: inline-block;
  background: rgba(0,0,0,0.3);
  padding: 10rpx 10rpx;
  border-radius: 40rpx;
}
.address {
  font-size: 30rpx;
  opacity: 0.8;
  margin-top: 10rpx;
}
.wayofask {
  display: flex;
  height: 200rpx;
  background: #fff;
  align-items: center;
}
.wayofask text {
  display: inline-block;
  position: relative;
  flex: 1;
  text-align: center;
  height: 50rpx;
  transform: translateY(-25rpx);
}
.wayofask text::before {  
  position: absolute;
  content: attr(data-msg);
  top: 100%;
  opacity: 0.8;
  color: #999;
  text-align: center;
  height: 50rpx;
  margin-top: 10rpx;
  font-size: 30rpx;
  left: 0;
  right: 0;
}
.body {
  margin-top: 30rpx;
  padding: 0 40rpx;
  background: #fff;
}
